<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>ServletNow</servlet-name>
  	<display-name>ServletNow</display-name>
  	<description></description>
  	<servlet-class>com.rahul.demo.ServletNow</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServletDemo</servlet-name>
  	<display-name>ServletDemo</display-name>
  	<description></description>
  	<servlet-class>com.rahul.demo.ServletDemo</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>ServeDemo</servlet-name>
  	<display-name>ServeDemo</display-name>
  	<description></description>
  	<servlet-class>demo.ServeDemo</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>ServletNow</servlet-name>
  	<url-pattern>/ServletNow</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServletDemo</servlet-name>
  	<url-pattern>/ServletDemo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>ServeDemo</servlet-name>
  	<url-pattern>/ServeDemo</url-pattern>
  </servlet-mapping>
</web-app>
